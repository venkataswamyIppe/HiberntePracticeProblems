package com.hibernate;

import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;

@NamedQueries( 
		{  
	        @NamedQuery(  
	        name = "findEmployeeByName",  
	        query = "from Employee e where e.name = :name"  
	        )  
	    }  
	)  
public class Employee {
	
	private int id;  
    private String name;  
    private int salary;

}
